# RealSenseToPC CMake config file
#
# This file sets the following variables:
# RealSenseToPC_FOUND - Always TRUE.
# RealSenseToPC_INCLUDE_DIRS - Directories containing the RealSenseToPC include files.
# RealSenseToPC_IDL_DIRS - Directories containing the RealSenseToPC IDL files.
# RealSenseToPC_LIBRARIES - Libraries needed to use RealSenseToPC.
# RealSenseToPC_DEFINITIONS - Compiler flags for RealSenseToPC.
# RealSenseToPC_VERSION - The version of RealSenseToPC found.
# RealSenseToPC_VERSION_MAJOR - The major version of RealSenseToPC found.
# RealSenseToPC_VERSION_MINOR - The minor version of RealSenseToPC found.
# RealSenseToPC_VERSION_REVISION - The revision version of RealSenseToPC found.
# RealSenseToPC_VERSION_CANDIDATE - The candidate version of RealSenseToPC found.

message(STATUS "Found RealSenseToPC-@PROJECT_VERSION@")
set(RealSenseToPC_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(RealSenseToPC_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(RealSenseToPC_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(RealSenseToPC_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(RealSenseToPC_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(RealSenseToPC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(RealSenseToPC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(RealSenseToPC_DEFINITIONS ${<dependency>_DEFINITIONS})

set(RealSenseToPC_VERSION @PROJECT_VERSION@)
set(RealSenseToPC_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(RealSenseToPC_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(RealSenseToPC_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(RealSenseToPC_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

